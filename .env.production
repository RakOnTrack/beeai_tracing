# Production Environment Configuration for Splunk SignalFX
# Copy this file to .env and update with your production values

# BeeAI Configuration
BEEAI_API_KEY=your_beeai_api_key_here

# Splunk SignalFX OTEL Configuration
OTEL_ENDPOINT=http://localhost:4328
SERVICE_NAME=beeai-gemini-agent
ENVIRONMENT=production

# Optional: Add authentication if required
# OTEL_HEADERS_AUTHORIZATION=Bearer your-token
# OTEL_HEADERS_X_SF_TOKEN=your-signalfx-token

# Model Configuration
BEEAI_MODEL=gemini-flash-1.5
BEEAI_TIMEOUT=30000
BEEAI_MAX_TOKENS=1000
